<Window x:Class="Microsoft.SqlServer.DatabaseInstaller.CancelConfirmDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Cancel Confirmation" Height="191" Width="367"
        Background="{DynamicResource CornerSplashImageBrush}">
    <Grid>
        <TextBlock Height="32" Margin="49,27,52,0" Name="ConfirmTextBlock" VerticalAlignment="Top">
                   <Run>Database installation is not complete.</Run>
            <LineBreak />
            <Bold>Are you sure you want to cancel?</Bold>
        </TextBlock>
        <Button Height="27" Margin="93,0,0,8" Name="NoButton" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="104" Click="NoButton_Click">No, go back</Button>
        <Button Height="27" HorizontalAlignment="Right" Margin="0,0,12,8" Name="YesButton" VerticalAlignment="Bottom" Width="104" Click="YesButton_Click">Yes</Button>
    </Grid>
</Window>
